<div class="row">
  <div class="col-md-12">
    <h2>{{ data.title }}</h2>
  </div>
  <div class="col-md-12">
    <h2>{{ data.content|raw }}</h2>
  </div>
</div>
<section class="timeline">
  <div class="container">
    {% for key, timeLine in timeLines %}
    <div class="timeline-item">
      <div class="timeline-img"></div>

      <div class="timeline-content js--{{ key is odd ? 'fadeInLeft' : 'fadeInRight'}}">
        <div class="date">{{ timeLine.year }}</div>
        <p>{{ timeLine.title }}</p>
      </div>
    </div>
    {% endfor %}




  </div>
</section>
