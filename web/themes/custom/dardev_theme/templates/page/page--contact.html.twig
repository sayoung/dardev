{#
/**
 * @file
 * Dardev's theme implementation to display a single Drupal page.
 */
#}
{% set has_breadcrumb = '' %}
<div class="page" id="top">
  {% include header_skin %}

  <main id="main">

    {% if page.breadcrumbs %}
      {% set has_breadcrumb = ' has-breadcrumb' %}
        {{ page.breadcrumbs }}
    {% endif %}

    {% if page.contact %}
      {{ page.contact }}
    {% endif %}


  </main>


  {% if page.after_content %}
    <div class="area after-content">
      <div class="container">
        <div class="content-inner">
          {{ page.after_content }}
        </div>
      </div>
    </div>
  {% endif %}


  {% include '@dardev_theme/page/footer.html.twig' %}
</div>
